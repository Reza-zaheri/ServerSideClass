<table dir="rtl">
    <tr>
        <th>کد کاربر</th>
        <th>نام</th>
        <th>نام خانوادگی</th>
        <th>تاریخ تولد</th>
        <th>ایمیل</th>
        <th>تلفن</th>
    </tr>
    @{
        foreach(var item in Model)
        {
            <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Family</td>
            <td>@item.Birthdaysh</td>
            <td>@item.Email</td>
            <td>@item.Phone</td>
            </tr>
        }
    }
</table>
